{
  "name": "surge-mobility-backoffice-call-center",
  "version": "0.0.1",
  "license": "MIT",
  "resolutions": {
    "webpack": "^5.0.0"
  },
  "scripts": {
    "ng": "ng",
    "conventional-changelog": "conventional-changelog",
    "start": "ng serve",
    "start:prod": "ng serve --configuration production",
    "start:stage": "ng serve --configuration staging",
    "start:local": "ng serve --configuration local",
    "start:development-surge": "ng serve --configuration development-surge",
    "build": "ng build",
    "build:prod": "npm run build -- --configuration production --aot",
    "build:stage": "npm run build -- --configuration staging --aot",
    "build:local": "npm run build -- --configuration local --aot",
    "build:development-surge": "npm run build -- --configuration development-surge --aot",
    "build-api": "rm -rf ./api/api.ts && rm -rf ./api/model && openapi-generator-cli generate -o ./api -i https://surge-mobility-api-dev.azurewebsites.net/swagger/v1/swagger.json -g typescript-angular --additional-properties useEnumExtension=true,modelPropertyNaming=original,enumValueNaming=UPPERCASE",
    "test": "ng test",
    "test:coverage": "rimraf coverage && npm run test -- --code-coverage",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "e2e": "ng e2e",
    "docs": "compodoc -p src/tsconfig.app.json -d docs",
    "docs:serve": "compodoc -p src/tsconfig.app.json -d docs -s",
    "release:changelog": "npm run conventional-changelog -- -p angular -i CHANGELOG.md -s"
  },
